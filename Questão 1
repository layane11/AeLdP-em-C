#include <stdio.h>

int main() {
    int numero;
    printf("Digite um numero de 5 digitos: ");
    scanf("%d", &numero);

    int n1 = numero / 10000;
    int n2 = (numero / 1000) % 10;
    int n3 = (numero / 100) % 10;
    int n4 = (numero / 10) % 10;
    int n5 = numero % 10;

    printf("%d   %d   %d   %d   %d\n", n1, n2, n3, n4, n5);
    return 0;
}
