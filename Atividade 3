#include <stdio.h>

int main() {
    short idade;
    int deposito;
    char conta[30];

    printf("Digite sua idade: ");
    scanf("%hd", &idade);

    printf("Digite o valor a depositar: ");
    scanf("%d", &deposito);

    printf("Digite o numero da conta: ");
    scanf("%s", conta);

    printf("\nIdade: %hd anos\n", idade);
    printf("Valor depositado: %d\n", deposito);
    printf("Conta: %s\n", conta);

    return 0;
}
